<h1>Login Page</h1>
<%=  form_for @user, url: login_path, :post do |f| %>
  <% flash[:errors] && flash[:errors].each do |err| %>
<p><%= err  %> </p>
<% end %>
<div>
  <%= f.label :username  %>
<%= f.text_field :username %>
</div>

<div>
  <%= f.label :password %>
  <%= f.password_field :password %>
</div>

<div>
  <%= f.submit "Login" %>
</div>
<% end %>
